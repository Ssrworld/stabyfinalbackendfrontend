{
  "name": "stabylink-backend",
  "version": "1.0.0",
  "description": "Backend server for Stabylink",
  "main": "server.js",
  "scripts": {
    "start": "concurrently \"node server.js\" \"npm:automate\"",
    "dev": "nodemon --signal SIGTERM server.js",
    "automate": "node src/scripts/masterRunner.js",
    "db:migrate": "npx knex migrate:latest",
    "db:rollback": "npx knex migrate:rollback",
    "db:make": "npx knex migrate:make",
    "db:seed": "node src/scripts/seedUsers.js",
    "get-key": "node src/scripts/getPrivateKey.js",
    "export-wallets": "node src/scripts/exportUserWallets.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bip39": "^3.1.0",
    "cloudinary": "^1.41.3",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dompurify": "^3.2.6",
    "dotenv": "^16.3.1",
    "ethereumjs-wallet": "^1.0.2",
    "ethers": "^6.8.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "mysql2": "^3.14.3",
    "nanoid": "^5.1.5",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "pg": "^8.16.3",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}